{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "PRISMA_MIGRATE_SKIP_GENERATE": "false"
  },
  "buildEnv": {
    "DATABASE_URL": "${env.DATABASE_URL}"
  },
  "regions": ["gcp-us-central1"]
} 